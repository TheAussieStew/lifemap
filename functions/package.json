    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "eslint": "^8.54.0",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.5.0",
    "jsonwebtoken": "^9.0.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
